{
	"TIP1": {
		"component": "TimedInputPin",
		"data": {
			"gpioPort": 12,
			"out": [
			        {
			        	"target": "Pin1",
			        	"export": {
			        		"measurement": "time"
			        	}
			        }
		        	]
		}
	},
	"Pin1": {
		"component": "Pin",
		"data": {
			"gpioPort": 7,
			"io": "output",
			"value": true,
			"input": {
				"time": 1000
			},
			"out": [
			        {
			        	"target": "Pin2"
			        }
		        	]
		}
	},
	"Pin2": {
		"component": "Pin",
		"data": {
			"gpioPort": 7,
			"io": "output",
			"value": false,
			"input": {
				"time": 1000
			},
			"out": [
			        {
			        	"target": "Pin3"
			        }
		        	]
		}
	},
	"Pin3": {
		"component": "Pin",
		"data": {
			"gpioPort": 7,
			"io": "output",
			"value": true,
			"input": {
				"time": 1000
			},
			"out": [
			        {
			        	"target": "Pin4"
			        }
		        	]
		}
	},
	"Pin4": {
		"component": "Pin",
		"data": {
			"gpioPort": 11,
			"io": "input",
			"out": [
			        {
			        	"target": "Pin1",
			        	"onChange": true
			        }
		        	]
		}
	}
}